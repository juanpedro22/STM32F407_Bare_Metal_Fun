ENTRY(Reset_Handler)

_estack = 0x20020000; /* RAM topo (128KB RAM) */

MEMORY
{
  FLASH (rx) : ORIGIN = 0x08000000, LENGTH = 512K
  RAM   (rwx): ORIGIN = 0x20000000, LENGTH = 128K
}

SECTIONS {
  .text : {
    KEEP(*(.isr_vector))
    *(.text*)
    *(.rodata*)
  } > FLASH

  .bss : {
    *(.bss*)
    *(COMMON)
  } > RAM

  .data : {
    *(.data*)
  } > RAM AT > FLASH
}
